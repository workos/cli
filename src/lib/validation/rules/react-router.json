{
  "framework": "react-router",
  "packages": [
    { "name": "@workos-inc/authkit-react-router", "location": "dependencies" },
    { "name": "react-router", "location": "any" }
  ],
  "envVars": [
    { "name": "WORKOS_API_KEY" },
    { "name": "WORKOS_CLIENT_ID" },
    { "name": "WORKOS_REDIRECT_URI" },
    { "name": "WORKOS_COOKIE_PASSWORD" }
  ],
  "files": [
    {
      "path": "app/routes/**/*callback*.{ts,tsx,js,jsx}",
      "mustContainAny": ["handleCallback", "authLoader", "@workos-inc/authkit-react-router"]
    },
    {
      "path": "app/**/*.{ts,tsx,js,jsx}",
      "mustContainAny": ["AuthKitProvider", "getSignInUrl", "getUser"]
    }
  ],
  "variants": {
    "v7-framework": {
      "packages": [],
      "envVars": [],
      "files": [
        {
          "path": "react-router.config.{ts,js,tsx,jsx}",
          "mustContain": []
        },
        {
          "path": "app/routes/**/*callback*.{ts,tsx}",
          "mustContainAny": ["loader", "handleCallback"]
        }
      ]
    },
    "v7-data": {
      "packages": [],
      "envVars": [],
      "files": [
        {
          "path": "src/**/*.{ts,tsx}",
          "mustContainAny": ["createBrowserRouter", "RouterProvider"]
        }
      ]
    }
  }
}
