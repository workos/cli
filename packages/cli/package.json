{
  "name": "@workos/authkit-wizard",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI wizard for automatically setting up WorkOS AuthKit in your project",
  "keywords": [
    "workos",
    "authkit",
    "authentication",
    "wizard",
    "cli",
    "setup",
    "install",
    "oauth",
    "sso"
  ],
  "bin": {
    "wizard": "dist/bin.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/bin.*",
    "dist/src",
    "dist/settings.config.*",
    ".claude-plugin",
    "skills",
    "package.json",
    "README.md"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "~0.2.12",
    "@clack/core": "^0.5.0",
    "@clack/prompts": "0.11.0",
    "@workos-inc/node": "^8.0.0",
    "chalk": "^5.6.2",
    "fast-glob": "^3.3.3",
    "ink": "^6.6.0",
    "opn": "^5.4.0",
    "react": "^19.2.3",
    "semver": "^7.7.3",
    "uuid": "^13.0.0",
    "xstate": "^5.25.1",
    "yargs": "^18.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/types": "~7.21.4",
    "@statelyai/inspect": "^0.4.0",
    "@types/chai": "^4.3.17",
    "@types/node": "^22.19.7",
    "@types/opn": "5.1.0",
    "@types/react": "^19.2.8",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.7",
    "@types/yargs": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitest/ui": "^4.0.17",
    "dotenv": "^16.4.7",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "msw": "^2.10.4",
    "prettier": "^3.8.0",
    "rimraf": "^3.0.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "pnpm clean",
    "build:watch": "pnpm tsc -w",
    "build": "pnpm tsc",
    "postbuild": "chmod +x ./dist/bin.js && cp -r scripts/** dist",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "format": "prettier --write .",
    "lint:prettier": "prettier --check \"{lib,src,test}/**/*.ts\"",
    "lint:eslint": "eslint . --cache --format stylish",
    "fix": "pnpm fix:eslint && pnpm fix:prettier",
    "fix:prettier": "prettier --write \"{lib,src,test}/**/*.ts\"",
    "fix:eslint": "eslint . --format stylish --fix",
    "try": "tsx dev.ts",
    "dev": "pnpm build && pnpm link --global && pnpm build:watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "pnpm fix"
  },
  "author": "WorkOS",
  "license": "MIT"
}
