{
  "name": "workos",
  "version": "0.3.2",
  "type": "module",
  "description": "The Official Workos CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/workos/cli"
  },
  "keywords": [
    "workos",
    "authkit",
    "authentication",
    "wizard",
    "cli",
    "setup",
    "install",
    "oauth",
    "sso"
  ],
  "bin": {
    "workos": "dist/bin.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    ".claude-plugin",
    "skills",
    "package.json",
    "README.md"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "~0.2.19",
    "@anthropic-ai/sdk": "^0.71.2",
    "@clack/core": "^0.5.0",
    "@clack/prompts": "0.11.0",
    "chalk": "^5.6.2",
    "diff": "^8.0.3",
    "fast-glob": "^3.3.3",
    "ink": "^6.6.0",
    "opn": "^5.4.0",
    "react": "^19.2.3",
    "semver": "^7.7.3",
    "uuid": "^13.0.0",
    "xstate": "^5.25.1",
    "yargs": "^18.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@statelyai/inspect": "^0.4.0",
    "@types/node": "~22.19.7",
    "@types/opn": "5.1.0",
    "@types/react": "^19.2.9",
    "@types/semver": "^7.7.1",
    "@types/yargs": "^17.0.35",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "dotenv": "^17.2.3",
    "prettier": "^3.8.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.20"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "pnpm clean",
    "build:watch": "pnpm tsc -w",
    "build": "pnpm tsc",
    "postbuild": "chmod +x ./dist/bin.js && cp -r scripts/** dist",
    "lint": "prettier --check \"{lib,src,test}/**/*.ts\"",
    "format": "prettier --write .",
    "try": "tsx dev.ts",
    "dev": "pnpm build && pnpm link --global && pnpm build:watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "author": "WorkOS",
  "license": "MIT"
}
